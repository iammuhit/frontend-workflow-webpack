{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["warn", "always"],
        "quotes": ["error", "single"],
        "indent": ["error", 4],
        "no-unused-vars": "off"
    },
    "overrides": []
}